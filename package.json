{
  "name": "gulp-tasks",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./ --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./ --include-path ./node_modules src/ -o src/ --watch --recursive --usePolling --polling-interval 1000",
    "start": "npm-run-all -p watch-css"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "browser-sync": "^2.16.0",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-clean-css": "^2.0.13",
    "gulp-concat": "^2.6.0",
    "gulp-debug": "^2.1.2",
    "gulp-if": "^2.0.1",
    "gulp-less": "^3.1.0",
    "gulp-sourcemaps": "^2.5.1",
    "gulp-uglify": "^2.0.0",
    "run-sequence": "^1.2.2",
    "chokidar": "^2.0.4",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3"
  }
}
